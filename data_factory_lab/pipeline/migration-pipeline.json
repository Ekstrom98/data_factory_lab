{
	"name": "migration-pipeline",
	"properties": {
		"activities": [
			{
				"name": "API_to_Datalake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['time']['updated']"
								}
							},
							{
								"source": {
									"path": "$['time']['updatedISO']"
								}
							},
							{
								"source": {
									"path": "$['time']['updateduk']"
								}
							},
							{
								"source": {
									"path": "$['disclaimer']"
								}
							},
							{
								"source": {
									"path": "$['chartName']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['USD']['code']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['USD']['symbol']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['USD']['rate']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['USD']['description']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['USD']['rate_float']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['GBP']['code']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['GBP']['symbol']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['GBP']['rate']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['GBP']['description']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['GBP']['rate_float']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['EUR']['code']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['EUR']['symbol']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['EUR']['rate']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['EUR']['description']"
								}
							},
							{
								"source": {
									"path": "$['bpi']['EUR']['rate_float']"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "API",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Datalake",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}